{
  "name": "react-transition-components",
  "version": "4.2.0",
  "author": "Setsun <setsun@setsun.io>",
  "license": "MIT",
  "description": "Easily configurable React components for animations / transitions ðŸ’ƒ",
  "repository": "https://github.com/Setsun/react-transition-components",
  "main": "dist/node/index.js",
  "browser": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "engines": {
    "npm": "^6.0.0"
  },
  "keywords": [
    "react",
    "react-component",
    "react-transition-group",
    "transition",
    "animation"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack --config webpack.config.js --watch",
    "build": "cross-env npm run build:browser && npm run build:node && tsc",
    "build:browser": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:node": "cross-env NODE_ENV=production TARGET_ENV=node webpack --config webpack.config.js",
    "test": "jest src/*",
    "typecheck": "tsc --noEmit --jsx react src/*",
    "typegen": "tsc",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@storybook/addon-docs": "5.2.3",
    "@storybook/addon-knobs": "5.2.3",
    "@storybook/core": "5.2.3",
    "@storybook/react": "5.2.3",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/jest": "^24.0.18",
    "babel-eslint": "^10.0.3",
    "babel-loader": "8.0.6",
    "babel-preset-setsun": "2.4.4",
    "cross-env": "6.0.3",
    "dotenv": "8.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.0",
    "eslint": "^6.5.1",
    "eslint-config-setsun": "2.4.0",
    "html-loader": "^0.5.5",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react": "16.8.6",
    "react-dom": "16.10.2",
    "react-transition-group": "4.2.1",
    "typescript": "^3.6.4",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-transition-group": ">=2.0.0",
    "react": "*",
    "react-transition-group": ">=2.0.0"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./.jest/setup.js"
    ]
  },
  "sideEffects": false
}
