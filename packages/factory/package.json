{
  "name": "react-transition-factory",
  "version": "2.0.0-alpha.2",
  "description": "Simple transition / animation component creation for React",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Setsun/react-transition-factory"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^5.0.0"
  },
  "keywords": [
    "react",
    "react-component",
    "react-transition-group",
    "transition",
    "animation",
    "staggered"
  ],
  "scripts": {
    "dev:client":
      "cross-env NODE_ENV=development webpack --config webpack.config.js --watch",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "precommit": "lint-staged",
    "eslint-prettier-check":
      "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "lint-staged": {
    "*.{js,json,css}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "@storybook/react": "^3.3.12",
    "@storybook/storybook-deployer": "^2.2.0",
    "babel-cli": "^7.0.0-beta.1",
    "babel-core": "^7.0.0-beta.1",
    "babel-eslint": "^8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^2.0.0-beta.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^7.0.0-beta.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^1.1.6",
    "cross-env": "^5.1.3",
    "css-loader": "0.28.9",
    "dotenv": "5.0.0",
    "eslint": "^4.17.0",
    "eslint-config-eslint": "^4.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "7.6.1",
    "file-loader": "1.1.6",
    "flow-bin": "^0.64.0",
    "husky": "^0.14.3",
    "lerna": "^2.5.1",
    "lint-staged": "^6.1.0",
    "nodemon": "^1.14.12",
    "office-ui-fabric-react": "^5.45.3",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-transition-group": "^2.2.1",
    "source-map-support": "^0.5.3",
    "style-loader": "0.20.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-transition-group": "^2.2.1"
  },
  "dependencies": {
    "lodash.camelcase": "^4.3.0"
  }
}
